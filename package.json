{
  "name": "AizBlog",
  "version": "1.0.0",
  "description": "Aiz personal blog",
  "author": "aizaiz",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
